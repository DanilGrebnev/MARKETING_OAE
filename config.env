# Конфигурация домена
DOMAIN=your-domain.com

# Настройки FastAPI
FASTAPI_HOST=0.0.0.0
FASTAPI_PORT=8000

# Настройки Nginx
NGINX_HTTP_PORT=80
NGINX_HTTPS_PORT=443

# Пути к SSL сертификатам (для продакшена)
SSL_CERT_PATH=./nginx/ssl/fullchain.pem
SSL_KEY_PATH=./nginx/ssl/privkey.pem

# Режим работы (development/production)
ENVIRONMENT=production
